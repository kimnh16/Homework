        var str = self.location.href;

	if ('http://abeek.incheon' == str.substring(0, 20)) {
            self.location.href = 'https://abeek.inu.ac.kr';
        }
        else if ('https://' != str.substring(0, 8)) {
            self.location.href = str.replace('http://', 'https://');
        }
        
        function CheckForm(id, pwd) {
            try {
                var agentObj = new ActiveXObject("EzIcd.BFS.1")
                if (document.getElementById(id).value == '') {
                    alert('아이디는 필수항목입니다.');
                    document.getElementById(id).focus();
                    return false;
                }
                else if (document.getElementById(pwd).value == '') {
                    alert('비밀번호는 필수항목입니다.');
                    document.getElementById(pwd).focus();
                    return false;
                }
                agentObj = null;
            } catch (e) { }
        }
        function Popup() {
            //window.open('/Contents.aspx');
            document.location.href = '/Contents.aspx';
            return false;
        }
        var str = self.location.href;
        if ('http://icee' == str.substring(0, 11))
        {
            document.location.href = '/OpenWeb/Center/';
        }
